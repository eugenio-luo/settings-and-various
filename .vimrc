syntax on
filetype plugin indent on
set tabstop=4
set shiftwidth=4
set expandtab
set guifont=Iosevka\ 20
"set guioptions-=m
"set guioptions-=T
"set noesckeys
set relativenumber
set number
set ignorecase
set smartcase
"set incsearch
set autochdir
"set cinoptions=l1
"colorscheme desert

"
" " `matchit.vim` is built-in so let's enable it!
" " Hit `%` on `if` to jump to `else`.
runtime macros/matchit.vim

set autoindent                 
set backspace=indent,eol,start 
set hidden                    
set incsearch                  
set ruler                     
set wildmenu                  
